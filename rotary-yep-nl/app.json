{
  "expo": {
    "name": "Rotary YEP NL",
    "slug": "rotary-yep-nl",
    "version": "11.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "rotaryyepnl",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "githubUrl": "https://github.com/Caeli-technologies/rotary_nl_rye",
    "platforms": [
      "ios",
      "android"
    ],
    "ios": {
      "buildNumber": "102",
      "appleTeamId": "A8S2486A44",
      "userInterfaceStyle": "automatic",
      "appStoreUrl": "https://apps.apple.com/nl/app/rotary-youth-exchange-nl/id1567096118",
      "supportsTablet": true,
      "bundleIdentifier": "com.caelitechnologies.rotary-nl-rye",
      "splash": {
        "image": "./assets/images/icon.png",
        "resizeMode": "contain"
      }
    },
    "android": {
      "versionCode": 102,
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.caelitechnologies.rotary_nl_rye"
    },
    "plugins": [
      "expo-router",
      "expo-system-ui",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}